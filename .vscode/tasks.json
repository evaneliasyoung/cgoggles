{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "command": "make",
      "args": [
        "-j12"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "clean",
      "type": "shell",
      "command": "rm",
      "args": [
        "-rf",
        "*/*.o",
        "*/*.exe"
      ],
      "group": "none",
      "problemMatcher": []
    },
    {
      "label": "clean-all",
      "type": "shell",
      "command": "rm",
      "args": [
        "-rf",
        "*/*.o",
        "*/*.gch",
        "*/*.exe"
      ],
      "group": "none",
      "problemMatcher": []
    }
  ]
}
